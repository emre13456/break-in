local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("SpaceGG | Break in", "Ocean")
local Tab = Window:NewTab("Break in")
local Section = Tab:NewSection("Break in")
Section:NewButton("Chips", "Gives You a Chips", function()
    game.ReplicatedStorage.RemoteEvents.GiveTool:FireServer("Chips")
end)

Section:NewButton("Bloxy Cola", "Gives You a  Bloxy Cola", function()
    game.ReplicatedStorage.RemoteEvents.GiveTool:FireServer("BloxyCola")
end)

Section:NewButton("Apple", "Gives You a Apple", function()
    game.ReplicatedStorage.RemoteEvents.GiveTool:FireServer("Apple")
end)

Section:NewButton("MedKit", "Gives You a MedKit", function()
    game.ReplicatedStorage.RemoteEvents.GiveTool:FireServer("MedKit")
end)
Section:NewButton("Cookie", "Gives You a Cookie", function()
    game.ReplicatedStorage.RemoteEvents.GiveTool:FireServer("Cookie")
end)

Section:NewButton("Teddy", "Gives a Teddy Bear", function()
    print("Clicked")
end)

Section:NewButton("Heal", "Heals You", function()
    for i = 1, 200 do
        wait(0.0001)
        local A_1 = "Cat"
        local Event = game:GetService("ReplicatedStorage").RemoteEvents.Energy
        Event:FireServer(A_1)
    end
end)

Section:NewButton("Remove Tool", "Removes All Of Your Tools", function()
    for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
        if v:IsA("Tool") then
            v:Destroy()
        end
    end
end)

Section:NewButton("Super Heal", "Gives u the Super Heal", function()
    game.ReplicatedStorage.RemoteEvents.GiveTool:FireServer("Cure")
end)

Section:NewButton("Plank", "Gives you a Plank", function()
    game.ReplicatedStorage.RemoteEvents.GiveTool:FireServer("Plank")
end)

Section:NewButton("Lollipop", "Gives you a Lollipop", function()
    game.ReplicatedStorage.RemoteEvents.GiveTool:FireServer("Lollipop")
end)

local Tab = Window:NewTab("Teleportings")
local Section = Tab:NewSection("Teleportings")


Section:NewButton("House", "Teleports You To House", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-36, 3, -200)
end)

Section:NewButton("Basement", "Teleports You To Basement", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(71, -15, -163)
end)

Section:NewButton("Attic", "Teleports You To Attic", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-16, 35, -220)
end)

Section:NewButton("Store", "Teleports You To Store", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-422, 3, -121)
end)

Section:NewButton("Sewer", "Teleports You To Sewer", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(129, 3, -125)
end)

Section:NewButton("Boss Room", "Teleports You To Boss Room", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-39, -287, -1480)
end)

local Tab = Window:NewTab("Local Player")
local Section = Tab:NewSection("Local Player")

Section:NewSlider("WalkSpeed", "Sets Ur WalkSpeed", 250, 16, function(s) 
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Section:NewSlider("JumpPower", "Sets Ur JumpPower", 350, 50, function(s) 
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
